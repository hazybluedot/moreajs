{
    "name": "morea-react",
    "scripts": {
        "deploy": "npm run build && scp public/bundle.min.js efcms:~/js/moreajs.min.js",
        "test": "browserify app/main.js -r ./deploy.js:config -o public/bundle.js --debug --verbose -t [ babelify --presets [ @babel/preset-env ] ] && scp public/bundle.js efcms:~/js/moreajs.test.js",
        "build": "browserify app/main.js -r ./deploy.js:config -t [ babelify --presets [ @babel/preset-env ] ] | uglifyjs > public/bundle.min.js",
        "watch": "watchify app/main.js -r ./local.js:config -o public/bundle.js --debug --verbose -t [ babelify --presets [ @babel/preset-env ] ]",
        "lint": "eslint --fix app/*.js"
    },
